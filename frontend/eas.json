{
  "cli": {
    "version": ">= 16.20.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "cache": {
        "key": "production-cache",
        "paths": ["node_modules", "android/.gradle"]
      }
    }
  },
  "submit": {
    "production": {}
  }
}